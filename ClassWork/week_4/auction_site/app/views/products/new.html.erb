<h1>Que Barato</h1>

<h3>Add new product</h3>

<%= form_for [@user, @product] do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>
	<%= f.label :description %>
	<%= f.text_area :description %>
	<%= f.label :deadline %>
	<%= f.datetime_local_field :deadline %>	

	<%= f.submit "Save" %>

<% end %>
	