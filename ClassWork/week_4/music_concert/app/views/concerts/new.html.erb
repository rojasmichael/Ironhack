<h1>Add a New Concert</h1>

<%= form_for @concert do |f| %>
	<%= f.label :artist %>
	<%= f.text_field :artist %>
	<%= f.label :venue %>
	<%= f.text_field :venue %>
	<%= f.label :city %>
	<%= f.text_field :city %>
	<%= f.label :price %>
	<%= f.text_field :price %>
	<%= f.label :date %>
	<%= f.datetime_local_field :date %>	
	<%= f.label :description %>
	<%=f.text_area :description %>

<%= f.submit "Save" %>

<% end %>